{
    "name": "webvariants/babelcache",
    "type": "library",
    "description": "Generic caching library with namespace support",
    "keywords": [
        "cache",
        "caching",
        "apc",
        "filesystem",
        "memcache",
        "memcached",
        "mysql",
        "redis",
        "sqlite",
        "xcache",
        "zendserver"
    ],
    "homepage": "http://docs.webvariants.de/babelcache/latest/",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Mewes",
            "email": "christoph@webvariants.de"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "apigen/apigen": "~2.8"
    },
    "suggest": {
        "predis/predis": "install this to use the Redis adapter"
    },
    "autoload": {
        "psr-0": {
            "wv\\BabelCache": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-default": "2.x-dev"
        }
    }
}
